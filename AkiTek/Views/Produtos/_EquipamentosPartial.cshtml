@model IEnumerable<AkiTek.Models.Equipamentos>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NumSerie)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model.Where(g => !g.Vendido)) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NumSerie)
        </td>
        <td>
            @Html.ActionLink("Remover", "Delete", "Equipamentos", new { id = item.ID, }, new { target = "_blank" })
        </td>
    </tr>
}

</table>
